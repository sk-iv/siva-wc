{
  "name": "root",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.js",
    "tsc:watch": "tsc --watch",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index docs/index.html --node-resolve --open --watch\"",
    "rollup": "rollup -c rollup.config.js",
    "serve": "serve",
    "dev": "webpack serve --env development --mode development",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@open-wc/eslint-config": "^7.0.0",
    "@testing-library/dom": "^8.11.1",
    "@types/jest": "^26.0.20",
    "@web/dev-server": "^0.1.29",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/dev-server-rollup": "^0.3.14",
    "autoprefixer": "^10.4.1",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.1.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-pug-plugin": "^4.0.0",
    "jest": "^27.4.7",
    "lerna": "^3.22.1",
    "lit-css-loader": "^1.2.2",
    "postcss": "^8.4.5",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "svelte": "^3.55.0",
    "svelte-loader": "^3.1.4",
