  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "effector": "^22.1.2",
    "js-custom-media": "^1.0.0",
    "lit": "^2.1.0",
    "page": "^1.11.6",
    "svelte-navigator": "^3.2.2",
    "tslib": "^2.1.0"
